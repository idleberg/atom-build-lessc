{
  "name": "build-lessc",
  "version": "0.7.2",
  "description": "Atom Build provider for lessc, compiles Less",
  "repository": "https://github.com/idleberg/atom-build-lessc",
  "license": "MIT",
  "keywords": [
    "buildprovider",
    "compile",
    "less",
    "lessc",
    "css",
    "linter",
    "lint"
  ],
  "main": "lib/provider.js",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
