  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^3.19.0",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-jshint": "^2.0.4",
    "gulp-jsonlint": "^1.2.0",
    "gulp-lesshint": "^3.0.1",
    "jshint": "^2.9.4"
  },
  "scripts": {
    "test": "gulp lint"
  }
